<UserControl x:Class="ElectricalCalculator.Symbols.Inductor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ElectricalCalculator.Symbols"
             mc:Ignorable="d" 
             Height="55" Width="30">
    <Canvas>
        <Line X1="20" Y1="0" X2="20" Y2="8" StrokeStartLineCap="Flat"/>
        <Path Data="M 20,7 A 4,2.5 180 0 0 20,17" StrokeStartLineCap="Round" StrokeThickness="2" Stroke="{StaticResource SchemNetColor}"/>
        <Path Data="M 20,17 A 4,2.5 180 0 0 20,27" StrokeStartLineCap="Round" StrokeThickness="2" Stroke="{StaticResource SchemNetColor}"/>
        <Path Data="M 20,27 A 4,2.5 180 0 0 20,37" StrokeStartLineCap="Round" StrokeThickness="2" Stroke="{StaticResource SchemNetColor}"/>
        <Path Data="M 20,37 A 4,2.5 180 0 0 20,47" StrokeStartLineCap="Round" StrokeThickness="2" Stroke="{StaticResource SchemNetColor}"/>
        <Line X1="20" Y1="46" X2="20" Y2="55" StrokeStartLineCap="Flat"/>
        <Line X1="8" Y1="8" X2="8" Y2="45"/>
        <Line X1="3" Y1="8" X2="3" Y2="45"/>
    </Canvas>
</UserControl>
