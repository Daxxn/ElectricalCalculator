<UserControl x:Class="ElectricalCalculator.Symbols.Resistor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ElectricalCalculator.Symbols"
             mc:Ignorable="d" x:Name="root" Width="20" Height="55">
    <UserControl.Resources>
        <Style TargetType="Line">
            <Setter Property="Stroke" Value="{StaticResource SchemNetColor}"/>
            <Setter Property="StrokeEndLineCap" Value="Round"/>
            <Setter Property="StrokeStartLineCap" Value="Round"/>
            <Setter Property="StrokeThickness" Value="2.5"/>
        </Style>
    </UserControl.Resources>
    <Canvas Width="Auto" Height="Auto">
        <Line X1="10" Y1="0" X2="10" Y2="10" StrokeStartLineCap="Flat"/>
        <Line X1="10" Y1="10" X2="0" Y2="15"/>
        <Line X1="0" Y1="15" X2="20" Y2="20"/>
        <Line X1="20" Y1="20" X2="0" Y2="25"/>
        <Line X1="0" Y1="25" X2="20" Y2="30"/>
        <Line X1="20" Y1="30" X2="0" Y2="35"/>
        <Line X1="0" Y1="35" X2="20" Y2="40"/>
        <Line X1="20" Y1="40" X2="10" Y2="45"/>
        <Line X1="10" Y1="45" X2="10" Y2="55" StrokeEndLineCap="Flat"/>
    </Canvas>
</UserControl>
